<%= form_for bookings_path, :html => {:id => "booking"} do |f| %>
	<div class = "field">
			<%= f.label :start_date %>
			<%= f.text_field :start_date %>
	</div>
	<div class = "field">
			<%= f.label :end_date %>
			<%= f.text_field :end_date %>
	</div>
	<div class = "actions"><%= f.submit value = "Create" %></div>
<% end %>